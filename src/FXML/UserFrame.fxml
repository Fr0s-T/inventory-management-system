                <Button fx:id="LogoutBtn" mnemonicParsing="false" prefHeight="80.0" prefWidth="248.0" styleClass="logout-button" text="Logout">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0">
                            <image>
                                <Image url="@../Icons/logout_icon.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </VBox>
      <HBox layoutX="960.0" layoutY="144.0" prefHeight="100.0" prefWidth="1293.0" AnchorPane.leftAnchor="257.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
             <Button id="ProductsBtn" fx:id="ProductsBtn" mnemonicParsing="false" prefHeight="130.0" prefWidth="430.0" text="Products" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="top-button" />
                  <String fx:value="products-button" />
               </styleClass><graphic>
                 <ImageView fitHeight="30.0" fitWidth="30.0">
                     <image>
                         <Image url="@/Icons/clothes.png" />
                     </image>
                 </ImageView>
             </graphic>
             </Button>
            <Button id="ShipmentsBtn" fx:id="ShipmentsBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="130.0" prefWidth="430.0" text="Shipments" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="shipments-button" />
                  <String fx:value="top-button" />
               </styleClass><graphic>
                <ImageView fitHeight="30.0" fitWidth="30.0">
                    <image>
                        <Image url="@/Icons/shipment.png" />
                    </image>
                </ImageView>
            </graphic></Button>
            <Button id="ReportsBtn" fx:id="ReportsBtn" layoutX="440.0" layoutY="10.0" mnemonicParsing="false" prefHeight="130.0" prefWidth="430.0" text="Reports" HBox.hgrow="ALWAYS">
