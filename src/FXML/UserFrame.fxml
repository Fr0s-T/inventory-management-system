<AnchorPane prefHeight="800.0" prefWidth="1550.0" stylesheets="@../StyleSheets/UserFrameStyleSheet.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.UserFrame">
    <children>
