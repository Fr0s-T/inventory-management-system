            <children>
                <Label alignment="CENTER" prefHeight="130.0" prefWidth="265.0">
                    <graphic>
                        <ImageView id="UserImage" fitHeight="130.0" fitWidth="274.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Icons/avatar-removebg-preview.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Label>

                <Label id="UsernameLabel" fx:id="UsernameLabel" alignment="CENTER" prefHeight="56.0" prefWidth="296.0" text="Username">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>

                <Label id="WarehouseLabel" fx:id="StatusLabel" alignment="CENTER" prefHeight="54.0" prefWidth="270.0" text="Warehouse">
